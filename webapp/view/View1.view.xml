<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" 	xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="UI5ChatBot.controller.View1">
	<App>
		<pages>
			<Page title=" AW ChatBot" enableScrolling="false">
				<content>
					<l:FixFlex fixContentSize="90%" >
		<l:fixContent>
			<List showSeparators="Inner" items="{/EntryCollection}" growing="true" growingThreshold="9"  >
		<FeedListItem sender="{Author}" icon="{AuthorPicUrl}" senderPress="onSenderPress" iconPress="onIconPress" iconDensityAware="false" info="{Type}" timestamp="{Date}" text="{Text}"/>
	</List>
		</l:fixContent>
		<l:flexContent>
			<FeedInput post="onPost" icon="/webapp/images/Gareth.jpg" class="sapUiSmallMarginTopBottom"/>
	
		</l:flexContent>
	</l:FixFlex>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>